{
  "name": "fhevm-sdk-project",
  "version": "1.0.0",
  "description": "Universal SDK for building dApps with Fully Homomorphic Encryption (FHE)",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "compile": "turbo run compile",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "deploy:contracts": "npm run compile --workspace=@fhevm/contracts && npm run deploy --workspace=@fhevm/contracts",
    "start:example": "npm run dev --workspace=example-dapp"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "fhe",
    "fhevm",
    "zama",
    "encryption",
    "privacy",
    "blockchain",
    "sdk",
    "web3"
  ],
  "author": "",
  "license": "MIT"
}
